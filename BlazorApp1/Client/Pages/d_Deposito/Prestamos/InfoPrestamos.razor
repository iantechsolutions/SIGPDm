@code {

    [Parameter] public int? Id { get; set; }
    Respuesta<PrestamoDTO> prestamo = new();

    protected override async Task OnInitializedAsync()
    {
        prestamo = await http.GetFromJsonAsync<Respuesta<PrestamoDTO>>($"/api/prestamo/{Id}");

    }
}
